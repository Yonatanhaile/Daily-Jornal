<%- include("partials/header") -%>

<h1>Delete Posts</h1>

<ul>
  <% posts.forEach(element => { %>
    <li>
      <h3> <%= element.title %></h3>
      <form action="/delete/<%= element._id %>" method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
      </form>
    </li>
  <% }); %>
</ul>

<%- include("partials/footer") -%>

